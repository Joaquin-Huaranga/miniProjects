---
import { processorsData } from '../../data-list';
import ComponentPage from '../../components/componentes-page/ComponentPage.astro'
import Layout from '../../layouts/Layout.astro'

export function getStaticPaths() {
  return Object.keys(processorsData).map((slug) => ({ params: { slug } }));
}

const { slug } = Astro.params;
const data = processorsData[slug];

if (!data) {
  return Astro.redirect('/404');   // ⬅️  ¡return, no throw!
}
---

<Layout>
  <ComponentPage data={data} />
</Layout>